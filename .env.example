# Django Configuration
DJANGO_SECRET_KEY=your-super-secret-key-here-change-this-min-50-characters
DJANGO_DEBUG=0
DJANGO_ALLOWED_HOSTS=.onrender.com,.vercel.app

# Frontend Configuration
FRONTEND_ORIGINS=https://your-vercel-app.vercel.app
CSRF_TRUSTED_ORIGINS=https://your-vercel-app.vercel.app

# Database (will be auto-provided by Render PostgreSQL)
DATABASE_URL=postgres://user:password@host:5432/database

# Redis (get from Upstash - https://upstash.com)
REDIS_URL=rediss://default:password@host:6379

# Server Port (auto-set by Render)
PORT=10000
